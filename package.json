{
    "name": "xpanse-ui",
    "version": "1.0.24",
    "private": true,
    "type": "module",
    "dependencies": {
        "@ant-design/icons": "^6.0.0",
        "@axa-fr/react-oidc": "^7.25.13",
        "@tanstack/react-query": "^5.77.2",
        "antd": "^5.25.1",
        "echarts": "5.6.0",
        "echarts-for-react": "^3.0.2",
        "rc-field-form": "^2.6.0",
        "rc-menu": "^9.16.0",
        "rc-tabs": "^15.5.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.6.0",
        "react-timer-hook": "^4.0.5",
        "yaml": "^2.8.0",
        "zustand": "^5.0.4"
    },
    "scripts": {
        "start": "vite --port 3000",
        "build": "vite build",
        "serve": "vite preview --port 3000",
        "start-with-zitadel-testbed": "env-cmd -f .env.zitadel-testbed vite --port 3000",
        "start-with-zitadel-local": "env-cmd -f .env.zitadel-local vite --port 3000",
        "build-with-zitadel-testbed": "env-cmd -f .env.zitadel-testbed vite build",
        "update-service-worker-file": "node ./node_modules/@axa-fr/oidc-client/bin/copy-service-worker-files.mjs public"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@axa-fr/oidc-client": "^7.22.32",
        "@eslint/compat": "^1.2.9",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.26.0",
        "@hey-api/openapi-ts": "^0.67.5",
        "@playwright/test": "^1.52.0",
        "@tanstack/eslint-plugin-query": "^5.78.0",
        "@types/react": "^19.1.4",
        "@types/react-dom": "^19.1.4",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "@vitejs/plugin-react": "^4.5.0",
        "env-cmd": "^10.1.0",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-import-resolver-typescript": "^4.4.1",
        "eslint-plugin-css-modules": "^2.12.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.4.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-require-explicit-generics": "^1.0.0",
        "knip": "^5.59.1",
        "playwright-core": "^1.50.1",
        "prettier": "3.5.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "release-it": "^19.0.2",
        "typescript": "5.8.3",
        "typescript-eslint": "^8.32.1",
        "uuid": "^11.1.0",
        "vite": "^6.3.5"
    },
    "overrides": {
        "zustand": {
            "use-sync-external-store": "^1.4.0"
        }
    },
    "knip": {
        "ignore": [
            "src/xpanse-api/**",
            "public/OidcServiceWorker.js",
            "public/inject.js",
            "src/config/index.d.ts",
            "src/react-app-env.d.ts",
            "public/OidcTrustedDomains.js",
            "openapi-ts.config.ts"
        ],
        "ignoreDependencies": [
            "@hey-api/openapi-ts",
            "eslint-plugin-react-hooks",
            "@typescript-eslint/eslint-plugin",
            "@typescript-eslint/parser",
            "eslint-config-prettier",
            "eslint-plugin-prettier",
            "@axa-fr/oidc-client"
        ],
        "eslint": {
            "config": [
                "./eslint.config.mjs"
            ]
        }
    },
    "release-it": {
        "github": {
            "release": true,
            "releaseNotes": "git log --no-merges --pretty=format:\"* %s %h\" ${latestTag}...main",
            "assets": [
                "*.zip"
            ]
        },
        "npm": {
            "publish": false
        },
        "hooks": {
            "before:init": [
                "git update-index -q --refresh"
            ],
            "after:bump": [
                "npm run update-service-worker-file",
                "npm run build",
                "zip -r -q xpanse-ui-${version}.zip dist"
            ]
        }
    }
}
